<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
    version="2.1">
	<managed-bean>
		<managed-bean-name>manageNationalite</managed-bean-name>
		<managed-bean-class>managedBeans.manageNationalite</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageSexe</managed-bean-name>
		<managed-bean-class>managedBeans.manageSexe</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>managePays</managed-bean-name>
		<managed-bean-class>managedBeans.ManagePays</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageEcole</managed-bean-name>
		<managed-bean-class>managedBeans.ManageEcole</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageDiplome</managed-bean-name>
		<managed-bean-class>managedBeans.ManageDiplome</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboEcole</managed-bean-name>
		<managed-bean-class>combo.ComboEcole</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageInscriptionConcours</managed-bean-name>
		<managed-bean-class>managedBeans.ManageInscriptionConcours</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboSexe</managed-bean-name>
		<managed-bean-class>combo.comboSexe</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboPays</managed-bean-name>
		<managed-bean-class>combo.ComboPays</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboNationalite</managed-bean-name>
		<managed-bean-class>combo.ComboNationalite</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageNiveaux</managed-bean-name>
		<managed-bean-class>managedBeans.ManageNiveaux</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboNiveau</managed-bean-name>
		<managed-bean-class>combo.ComboNiveau</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboDiplome</managed-bean-name>
		<managed-bean-class>combo.ComboDiplome</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageAnneeConcours</managed-bean-name>
		<managed-bean-class>managedBeans.ManageAnneeConcours</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageConcours</managed-bean-name>
		<managed-bean-class>managedBeans.ManageConcours</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>comboConcours</managed-bean-name>
		<managed-bean-class>combo.ComboConcours</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageAnneeScolaire</managed-bean-name>
		<managed-bean-class>managedBeans.ManageAnneeScolaire</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>manageInscription</managed-bean-name>
		<managed-bean-class>managedBeans.ManageInscription</managed-bean-class>
		<managed-bean-scope>view</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>templates/template.xhtml</display-name>
		<from-view-id>/templates/template.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/pages/inscriptionconcours.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/nationalite.xhtml</display-name>
		<from-view-id>/pages/nationalite.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>ok</from-outcome>
			<to-view-id>/pages/inscriptionconcours.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
