<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.artm.hibernate.beans">
	<class
		name="Paramvisite"
		table="paramvisite"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_PARAMA_VISITE"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="AnneeVisite"
			column="ANNEE_VISITE"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="MontantParamVisite"
			column="MONTANT_PARAM_VISITE"
			type="big_decimal"
			not-null="false"
			length="8"
		/>


	</class>	
</hibernate-mapping>