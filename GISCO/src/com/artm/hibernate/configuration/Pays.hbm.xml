<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.artm.hibernate.beans">
	<class
		name="Pays"
		table="pays"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODEPAYS"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			lazy="false"
			name="Libpays"
			column="LIBPAYS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Republic"
			column="REPUBLIC"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="Abrevpays"
			column="ABREVPAYS"
			type="string"
			not-null="false"
			length="25"
		/>
	

		<set name="Candidats" inverse="true" lazy="true">
			<key column="CODEPAYS"/>
			<one-to-many class="Candidat"/>
		</set>

		<set name="Etudiants" inverse="true" lazy="true">
			<key column="CODEPAYS"/>
			<one-to-many class="Etudiants"/>
		</set>

		<set name="Anneeconcours" inverse="true" lazy="true">
			<key column="CODEPAYS"/>
			<one-to-many class="Anneeconcours"/>
		</set>


	</class>	
</hibernate-mapping>