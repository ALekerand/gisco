<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" package="com.artm.hibernate.beans">
 <class name="Pays" table="pays">
  <meta attribute="sync-DAO">false</meta>
  <id column="CODEPAYS" name="Id" type="integer">
   <generator class="org.hibernate.id.IncrementGenerator"/>
  </id>
  <property column="LIBPAYS" generated="never" lazy="false" length="50"
   name="Libpays" type="string"/>
  <property column="REPUBLIC" generated="never" lazy="false" length="80"
   name="Republic" type="string"/>
  <property column="ABREVPAYS" generated="never" lazy="false"
   length="25" name="Abrevpays" type="string"/>
  <set inverse="true" lazy="true" name="Candidats" sort="unsorted">
   <key column="CODEPAYS"/>
   <one-to-many class="Candidat"/>
  </set>
  <set inverse="true" lazy="true" name="Etudiants" sort="unsorted">
   <key column="CODEPAYS"/>
   <one-to-many class="Etudiants"/>
  </set>
  <set inverse="true" lazy="true" name="Anneeconcours" sort="unsorted">
   <key column="CODEPAYS"/>
   <one-to-many class="Anneeconcours"/>
  </set>
 </class>
</hibernate-mapping>
