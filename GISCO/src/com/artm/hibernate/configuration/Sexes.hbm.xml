<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="false" package="com.artm.hibernate.beans">
 <class name="Sexes" table="sexes">
  <meta attribute="sync-DAO">false</meta>
  <id column="CODESEXE" name="Id" type="integer">
   <generator class="org.hibernate.id.IncrementGenerator"/>
  </id>
  <property column="LIBSEXE" generated="never" lazy="false" length="10"
   name="Libsexe" type="string"/>
  <set inverse="true" lazy="true" name="Candidats" sort="unsorted">
   <key column="CODESEXE"/>
   <one-to-many class="Candidat"/>
  </set>
  <set cascade="all" lazy="true" name="Etudiants" sort="unsorted" table="avoir">
   <key column="CODESEXE"/>
   <many-to-many class="Etudiants" column="NUMETUDIANT" unique="false"/>
  </set>
 </class>
</hibernate-mapping>
