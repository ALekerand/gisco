<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.artm.hibernate.beans">
	<class
		name="Candidat"
		table="candidat"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="NUM_CANDIDAT"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="NomCandidat"
			column="NOM_CANDIDAT"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="PrenomsCandidat"
			column="PRENOMS_CANDIDAT"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="DateNaissance"
			column="DATE_NAISSANCE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LieuNaissance"
			column="LIEU_NAISSANCE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="PhotoCandidat"
			column="PHOTO_CANDIDAT"
			type="string"
			not-null="false"
			length="150"
		/>
		<many-to-one
			name="Codenationalite"
			column="CODENATIONALITE"
			class="Nationalites"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Codepays"
			column="CODEPAYS"
			class="Pays"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Codesexe"
			column="CODESEXE"
			class="Sexes"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Codeniveau"
			column="CODENIVEAU"
			class="Niveaux"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeDiplome"
			column="CODE_DIPLOME"
			class="Diplomes"
			not-null="true"
		>
		</many-to-one>
	

		<set name="Inscriptionconcours" inverse="true" lazy="true">
			<key column="NUM_CANDIDAT"/>
			<one-to-many class="Inscriptionconcours"/>
		</set>

		<set name="Noteconcours" inverse="true" lazy="true">
			<key column="NUM_CANDIDAT"/>
			<one-to-many class="Noteconcours"/>
		</set>
	
		<set
			name="Salles"
			table="candidat_salle"
			cascade="all"
			lazy="true"
		>
			<key column="NUM_CANDIDAT"/>
			<many-to-many column="CODE_SALLE" class="Salles"/>
		</set>


	</class>	
</hibernate-mapping>