<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Correspondre"
		table="correspondre"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="CorrespondrePK">
			<key-many-to-one
				name="CodeSemestre"
				class="Semestre"
				column="CODE_SEMESTRE"
			/>
			<key-many-to-one
				name="CodeMatiere"
				class="Matiere"
				column="CODE_MATIERE"
			/>
		</composite-id>

		<property
			name="AnneeMatiereSemestre"
			column="ANNEE_MATIERE_SEMESTRE"
			type="string"
			not-null="false"
			length="10"
		/>
		<property
			name="DateMatiere"
			column="DATE_MATIERE"
			type="date"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>