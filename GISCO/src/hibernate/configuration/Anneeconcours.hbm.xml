<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Anneeconcours"
		table="anneeconcours"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_ANNEES_CONCOURS"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="AnneeConcoursDebut"
			column="ANNEE_CONCOURS_DEBUT"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="AnneeConcoursFin"
			column="ANNEE_CONCOURS_FIN"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="LibAnneeConcours"
			column="LIB_ANNEE_CONCOURS"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="DateComposition"
			column="DATE_COMPOSITION"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="DroitConcours"
			column="DROIT_CONCOURS"
			type="big_decimal"
			not-null="false"
			length="8"
		/>
		<property
			name="NbrePlaces"
			column="NBRE_PLACES"
			type="big_decimal"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Codepays"
			column="CODEPAYS"
			class="Pays"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeConcours"
			column="CODE_CONCOURS"
			class="Concours"
			not-null="true"
			lazy="false"
			
		>
		</many-to-one>
	

		<set name="Inscriptionconcours" inverse="true" lazy="true">
			<key column="CODE_ANNEES_CONCOURS"/>
			<one-to-many class="Inscriptionconcours"/>
		</set>

		<set name="Caisconcours" inverse="true" lazy="true">
			<key column="CODE_ANNEES_CONCOURS"/>
			<one-to-many class="Caisconcours"/>
		</set>


	</class>	
</hibernate-mapping>