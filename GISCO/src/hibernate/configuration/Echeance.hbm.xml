<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Echeance"
		table="echeance"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_ECHEANCE"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="MontantEchance"
			column="MONTANT_ECHANCE"
			type="big_decimal"
			not-null="false"
			length="8"
		/>
		<property
			name="DateEchance"
			column="DATE_ECHANCE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LibEcheance"
			column="LIB_ECHEANCE"
			type="string"
			not-null="false"
			length="20"
		/>
		<many-to-one
			name="CodeEcolage"
			column="CODE_ECOLAGE"
			class="Ecolages"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>