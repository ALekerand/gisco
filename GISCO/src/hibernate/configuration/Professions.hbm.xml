<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Professions"
		table="professions"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_PROFESSION"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="LibProfession"
			column="LIB_PROFESSION"
			type="string"
			not-null="false"
			length="50"
		/>
	

		<set name="Peres" inverse="true" lazy="true">
			<key column="CODE_PROFESSION"/>
			<one-to-many class="Peres"/>
		</set>

		<set name="Meres" inverse="true" lazy="true">
			<key column="CODE_PROFESSION"/>
			<one-to-many class="Meres"/>
		</set>


	</class>	
</hibernate-mapping>