<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Adresses"
		table="adresses"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_ADRESSE"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="BoitePostale"
			column="BOITE_POSTALE"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="Email"
			column="EMAIL"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="Tel1"
			column="TEL1"
			type="string"
			not-null="false"
			length="25"
		/>
		<property
			name="Tel2"
			column="TEL2"
			type="string"
			not-null="false"
			length="25"
		/>
	

		<set name="Tuteurs" inverse="true" lazy="true">
			<key column="CODE_ADRESSE"/>
			<one-to-many class="Tuteurs"/>
		</set>

		<set name="Peres" inverse="true" lazy="true">
			<key column="CODE_ADRESSE"/>
			<one-to-many class="Peres"/>
		</set>

		<set name="Meres" inverse="true" lazy="true">
			<key column="CODE_ADRESSE"/>
			<one-to-many class="Meres"/>
		</set>

		<set name="Etudiants" inverse="true" lazy="true">
			<key column="CODE_ADRESSE"/>
			<one-to-many class="Etudiants"/>
		</set>


	</class>	
</hibernate-mapping>