<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Etudiants"
		table="etudiants"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="NUMETUDIANT"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="Mle"
			column="MLE"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="Nom"
			column="NOM"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Prenoms"
			column="PRENOMS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Datenais"
			column="DATENAIS"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="Lieunais"
			column="LIEUNAIS"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="Nbenfants"
			column="NBENFANTS"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Nbsoeurs"
			column="NBSOEURS"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Nbfreres"
			column="NBFRERES"
			type="integer"
			not-null="false"
			length="10"
		/>
		<property
			name="Numcni"
			column="NUMCNI"
			type="string"
			not-null="false"
			length="30"
		/>
		<!-- please tell Joe Hudson that the type 'LONGBLOB' could not be resolved.. defaulting to java.lang.String -->
		<property
			name="Photo"
			column="PHOTO"
			type="java.lang.String"
			not-null="false"
		/>
		<many-to-one
			name="NumMere"
			column="NUM_MERE"
			class="Meres"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Codenationalite"
			column="CODENATIONALITE"
			class="Nationalites"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Codepays"
			column="CODEPAYS"
			class="Pays"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Nummere2"
			column="NUMMERE2"
			class="Tuteurs"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Codesante"
			column="CODESANTE"
			class="Santes"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="Codeniveau"
			column="CODENIVEAU"
			class="Niveaux"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeDiplome"
			column="CODE_DIPLOME"
			class="Diplomes"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="Codematrimoniale"
			column="CODEMATRIMONIALE"
			class="Matrimoniales"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeAdresse"
			column="CODE_ADRESSE"
			class="Adresses"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="NumPere"
			column="NUM_PERE"
			class="Peres"
			not-null="false"
		>
		</many-to-one>
	

		<set name="Inscriptions" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Inscriptions"/>
		</set>

		<set name="Scolarites" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Scolarites"/>
		</set>

		<set name="Notexams" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Notexam"/>
		</set>

		<set name="Absences" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Absence"/>
		</set>

		<set name="Versements" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Versement"/>
		</set>

		<set name="Notes" inverse="true" lazy="true">
			<key column="NUMETUDIANT"/>
			<one-to-many class="Notes"/>
		</set>
	
		<set
			name="Logements"
			table="loger"
			cascade="all"
			lazy="true"
		>
			<key column="NUMETUDIANT"/>
			<many-to-many column="CODE_LOGE" class="Logement"/>
		</set>
		<set
			name="Candidatures"
			table="etudiant_candidature"
			cascade="all"
			lazy="true"
		>
			<key column="NUMETUDIANT"/>
			<many-to-many column="CODE_CANDIDATURE" class="Candidature"/>
		</set>
		<set
			name="Sexes"
			table="avoir"
			cascade="all"
			lazy="true"
		>
			<key column="NUMETUDIANT"/>
			<many-to-many column="CODESEXE" class="Sexes"/>
		</set>


	</class>	
</hibernate-mapping>