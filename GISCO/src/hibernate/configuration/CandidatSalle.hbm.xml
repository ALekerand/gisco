<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="CandidatSalle"
		table="candidat_salle"
	>
		<meta attribute="sync-DAO">false</meta>
		<composite-id name="Id" class="CandidatSallePK">
			<key-many-to-one
				name="CodeSalle"
				class="Salles"
				column="CODE_SALLE"
			/>
			<key-many-to-one
				name="NumCandidat"
				class="Candidat"
				column="NUM_CANDIDAT"
			/>
		</composite-id>

		<property
			name="DateCandidatSalle"
			column="DATE_CANDIDAT_SALLE"
			type="date"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>