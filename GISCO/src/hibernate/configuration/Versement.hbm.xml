<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="hibernate.beans">
	<class
		name="Versement"
		table="versement"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="Id"
			type="integer"
			column="CODE_VERSEMENT"
		>
			<generator class="org.hibernate.id.IncrementGenerator"/>
		</id>

		<property
			name="DateCaisse"
			column="DATE_CAISSE"
			type="date"
			not-null="false"
			length="10"
		/>
		<property
			name="LibCaisse"
			column="LIB_CAISSE"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="MontantCaisse"
			column="MONTANT_CAISSE"
			type="big_decimal"
			not-null="false"
			length="8"
		/>
		<many-to-one
			name="Numetudiant"
			column="NUMETUDIANT"
			class="Etudiants"
			not-null="false"
		>
		</many-to-one>
		<many-to-one
			name="CodeAnnees"
			column="CODE_ANNEES"
			class="Annees"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeOriginr"
			column="CODE_ORIGINR"
			class="Origine"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeActivite"
			column="CODE_ACTIVITE"
			class="Activites"
			not-null="true"
		>
		</many-to-one>
		<many-to-one
			name="CodeMode"
			column="CODE_MODE"
			class="Mode"
			not-null="true"
		>
		</many-to-one>


	</class>	
</hibernate-mapping>